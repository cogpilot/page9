{
  "namespace": {
    "root": "/",
    "mounts": [
      {
        "path": "/data",
        "target": "/static/data",
        "type": "dir"
      },
      {
        "path": "/api/status",
        "target": "/status.json",
        "type": "file"
      }
    ]
  },
  "kernel": {
    "version": "0.1.0",
    "cachingStrategy": "cache-first",
    "interceptPatterns": [
      "/*"
    ]
  },
  "workers": {
    "enabled": true,
    "pool": {
      "min": 2,
      "max": 4
    },
    "modules": []
  },
  "routes": {
    "/": {
      "file": "/index.html",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    "/status": {
      "file": "/status.json",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  }
}
